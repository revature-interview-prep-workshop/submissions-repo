package com.revature.driver;

import com.revature.model.PLinkedList;

public class LinkedListDriver {
	public static void main(String[] args) {
		PLinkedList<Integer> list = new PLinkedList<Integer>();
		System.out.println(list);//empty list
		System.out.println(list.isEmpty());
		list.add(1);
		list.add(2);
		list.add(0,3);
		list.add(1,4);
		list.add(4,5);
		System.out.println(list);//Should be 3, 4, 1, 2, 5 here
		System.out.println(list.contains(3));//true
		System.out.println(list.contains(6));//false
		System.out.println(list.isEmpty());
	}
}
